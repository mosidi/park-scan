      "package": "com.rabab13.figma"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "80964fc0-9160-4241-b9dc-b860a510ba2f"
      }
    }
